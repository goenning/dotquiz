{
  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo": [
      { "Name": "LiterateConsole" },
      { "Name": "RollingFile", "Args": { "pathFormat": "./logs/log-{Date}.txt" } }
    ]
  },
  "Swagger": {
    "IsEnabled": true,
    "ApplicationInfo": [
      { "Version": "v1", "Title": "dotQuiz API v1" }
    ]
  }
}